geom_density(alpha = 0.5) +
theme_minimal() +
scale_x_continuous('Student travel time to school',
breaks = c(0,30,60,90,120,150),
labels = c('0:00', '0:30', '1:00','1:30','2:00','2:30')) +
ylab("Density") +
labs(title = "Distribution of Current Student Ride Times to School",
subtitle = "Students attending schools in chester compared to the the whole school system") +
scale_fill_manual(values = c("#7f7f7f", "#FF0000")) +
scale_color_manual(values = c("#7f7f7f", "#FF0000")) +
geom_vline(xintercept = avgs$ride_time, color = "white") +
annotate("text", label = "Districtwide Avg: 35.2 min", x = 33, y = 0.001, color = "white", angle = 90, hjust = 0, size = 3) +
theme(axis.title = element_text(hjust = 0.9, face = 'italic', size = 10),
legend.position = c(0.75, 0.5),
legend.title = element_blank())
